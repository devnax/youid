{
   "name": "youid",
   "version": "1.0.5",
   "description": "A lightweight utility to generate unique identifiers (UIDs) from strings or randomly.",
   "main": "./index.js",
   "module": "./esm/index.js",
   "types": "./types/index.d.ts",
   "type": "module",
   "author": {
      "name": "Naxrul Ahmed",
      "email": "devnaxrul@gmail.com"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/devnax/youid.git"
   },
   "scripts": {
      "start": "makepack start",
      "build": "makepack build",
      "build:publish": "makepack publish"
   },
   "devDependencies": {
      "@types/react": "^19.0.2",
      "@types/react-dom": "^19.0.2",
      "makepack": "^1.5.5",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "typescript": "^4.4.2"
   },
   "keywords": [
      "uid",
      "unique-id",
      "hash",
      "string-to-id",
      "random-id",
      "identifier"
   ],
   "exports": {
      ".": {
         "types": "./types/index.d.ts",
         "import": "./index.js",
         "require": "./cjs/index.js"
      },
      "./types/*": "./types/*.d.ts",
      "./cjs/*": "./cjs/*.js",
      "./*": {
         "import": "./*.js",
         "require": "./cjs/*.js"
      }
   }
}
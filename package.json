{
   "name": "youid",
   "version": "1.0.10",
   "description": "A lightweight utility to generate unique identifiers (UIDs) from strings or randomly.",
   "main": "./cjs/index.js",
   "module": "./index.js",
   "types": "./index.d.ts",
   "type": "module",
   "author": {
      "name": "Naxrul Ahmed",
      "email": "devnaxrul@gmail.com"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/devnax/youid.git"
   },
   "scripts": {
      "start": "makepack start",
      "build": "makepack build",
      "release": "makepack release"
   },
   "devDependencies": {
      "@types/react": "^19.0.2",
      "@types/react-dom": "^19.0.2",
      "makepack": "^1.6.8",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "typescript": "^4.4.2"
   },
   "keywords": [
      "uid",
      "unique-id",
      "hash",
      "string-to-id",
      "random-id",
      "identifier"
   ],
   "sideEffects": false,
   "exports": {
      ".": {
         "require": {
            "types": "./index.d.ts",
            "default": "./cjs/index.js"
         },
         "import": {
            "types": "./index.d.ts",
            "default": "./index.js"
         }
      },
      "./*": {
         "require": {
            "types": "./*.d.ts",
            "default": "./cjs/*.js"
         },
         "import": {
            "types": "./*.d.ts",
            "default": "./*.js"
         }
      },
      "./cjs": null
   }
}
